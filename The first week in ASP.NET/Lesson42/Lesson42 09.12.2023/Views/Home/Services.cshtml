@model List<Students>

<section class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-4">
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <h5 class="card-title">@item.Fullname</h5>
                    <h4 class="card-title">@item.Birthdate</h4>
                    <h3 class="card-title">@item.Grade</h3>
                    <a href="#" class="btn btn-primary">Read More</a>
                </div>
            </div>
        </div>
    }
</section>

